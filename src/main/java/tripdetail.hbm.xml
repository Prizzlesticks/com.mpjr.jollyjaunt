<?xml version='1.0' encoding='UTF-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
<class name="com.mpjr.jollyjaunt" table="trip_detail">
	<id name="trip_id" column="trip_id" type="int">
		<generator class="increment"></generator>
	</id>
	<property name="origin" column="origin" type="string"></property>
	<property name="destination" column="destination" type="string"></property>
	<property name="starting date" column="trip_startdate" type="string"></property>
	<property name="ending date" column="trip_enddate" type="string"></property>
	
	
</class>
</hibernate-mapping>