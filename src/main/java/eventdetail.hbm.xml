<?xml version='1.0' encoding='UTF-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<!-- hibernate-mapping to events table in MySQL -->
<hibernate-mapping>

	<class name="com.mpjr.jollyjaunt.EventDetail" table="events">
		<id name="eventid" column="eventid" type="int">
			<generator class="native"></generator>
		</id>
		<property name="event" column="event" type="string"></property>
		<property name="tripid" column="trip_id" type="int"></property>
		<property name="city" column="city" type="string"></property>
		<property name="date" column="date" type="string"></property>
	</class>

</hibernate-mapping>